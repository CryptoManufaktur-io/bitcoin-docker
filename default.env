# docker-compose files to use
COMPOSE_FILE=btc.yml
# Version and Bitcoin core signature to pull
BTCD_VERSION=0.21.1
BTCD_SIGNATURE=01EA5486DE18A882D4C2684590C8019E36C2E964
# For RPC access, user and password
RPC_USER=
RPC_PW=

# Secure web proxy - advanced use, please see instructions
DOMAIN=example.com
ACME_EMAIL=user@example.com
CF_EMAIL=user@example.com
CF_API_TOKEN=SECRETTOKEN
AWS_PROFILE=myprofile
AWS_HOSTED_ZONE_ID=myzoneid
BTC_HOST=btc
DDNS_SUBDOMAIN=
DDNS_PROXY=false

RESTART=unless-stopped

# Secure web proxy port, 443 and 80 are great defaults
TRAEFIK_WEB_PORT=443
TRAEFIK_WEB_HTTP_PORT=80
